@model CASEPro.Models.DOCUMENTO

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>DOCUMENTO</legend>

    <div class="display-label">NOMBRE</div>
    <div class="display-field">@Model.NOMBRE</div>

    <div class="display-label">TIPO</div>
    <div class="display-field">@Model.TIPO</div>

    <div class="display-label">UBICACION</div>
    <div class="display-field">@Model.UBICACION</div>

    <div class="display-label">REQUERIMIENTO_ID</div>
    <div class="display-field">@String.Format("{0:F}", Model.REQUERIMIENTO_ID)</div>

    <div class="display-label">REQUERIMIENTO</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.REQUERIMIENTO).ToString()</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


